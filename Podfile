# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'esca' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for esca
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'Firebase/Crash'
  pod 'FirebaseUI', '~> 1.0'
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'SideMenu'
  pod 'SwiftyJSON'
end
